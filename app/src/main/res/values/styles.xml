<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="Theme.beginBackground" parent="@android:style/Theme.Translucent.NoTitleBar" />

    <style name="ThemeSplash" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:background">@drawable/bg</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowFullscreen">true</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    <style name="line">
        <item name="android:layout_height">@dimen/dp_1</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:background">@color/color_e5e5e5</item>
    </style>

    <style name="interval">
        <item name="android:layout_height">@dimen/dp_10</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:background">@color/color_e5e5e5</item>
    </style>
</resources>
